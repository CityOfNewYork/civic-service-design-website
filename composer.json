{
  "name": "nyco/service-design",
  "version": "0.0.1",
  "description": "The Service Design Studio makes public services more effective and accessible for all New Yorkers",
  "homepage": "https://nyc.gov/servicdesign",
  "authors": [
    {
      "name": "We Are Immediate",
      "email": "info@immediate.nyc",
      "homepage": "https://weareimmediate.com"
    },
    {
      "name": "NYC Opportunity",
      "email": "products@nycopportunity.nyc.gov",
      "homepage": "http://nyc.gov/opportunity"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "nyco/wp-assets": "^1.4",
    "nyco/wp-config": "^1",
    "wpackagist-plugin/aryo-activity-log": "*",
    "wpackagist-plugin/google-authenticator": "*",
    "wpackagist-plugin/limit-login-attempts-reloaded": "*",
    "wpackagist-plugin/loggedin": "*",
    "wpackagist-plugin/wordpress-importer": "*",
    "wpackagist-plugin/members": "*",
    "wpackagist-plugin/wps-hide-login": "*",
    "wpackagist-plugin/wpscan": "*"
  },
  "require-dev": {
    "filp/whoops": "*",
    "squizlabs/php_codesniffer": "*",
    "whoops/soap": "*",
    "wpackagist-plugin/query-monitor": "*",
    "wpackagist-plugin/redis-cache": "*",
    "wpackagist-plugin/wp-crontrol": "*"
  },
  "scripts": {
    "development": [
      "composer dump-autoload"
    ],
    "production": [
      "composer dump-autoload --no-dev --optimize",
      "vendor/bin/phpcs --standard=phpcs.xml"
    ],
    "predeploy": [
      "composer dump-autoload --no-dev --optimize",
      "vendor/bin/phpcs --standard=phpcs.xml"
    ],
    "lint": [
      "vendor/bin/phpcs -s --standard=phpcs.xml"
    ],
    "fix": [
      "vendor/bin/phpcbf --standard=phpcs.xml"
    ],
    "version": [
      "composer update nothing",
      "composer dump-autoload --no-dev --optimize"
    ],
    "deps": [
      "composer show --tree"
    ]
  }
}
